{
  "name": "adcart",
  "vendor": "adcartbr",
  "version": "1.2.0-beta.0",
  "title": "Integração Adcart",
  "description": "Integre sua loja com a Adcart e recupere carrinhos abandonados anunciando em nossos sites parceiros.",
  "billingOptions": {
    "termsURL": "https://www.adcart.com.br/termos",
    "support": {
      "url": "https://www.adcart.com.br/"
    },
    "free": true,
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "builders": {
    "react": "3.x",
    "pixel": "0.x",
    "store": "0.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "AdCart",
    "type": "object",
    "properties": {
      "advertiserId": {
        "title": "ID da conta",
        "description": "ID da sua conta na Adcart.",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
